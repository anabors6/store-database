Insert into 'store_inventory' values (15,'Boys pants',56);
Insert into 'store_inventory' values (16,'Boys shorts',64);

select * from store_inventory
order by quantity asc; 

create table store_sales (ID Integer primary key, Item Integer,Percentage integer); 
select * from store_sales;
